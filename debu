int main()
{
        ios_base::sync_with_stdio(false), cin.tie(NULL);
        ll t;
        cin>>t;
        while(t--)
        {
             ll a,b;
             cin>>a>>b;
             ll k=abs(a-b);
             ll ans=(k/10);
             if(k%10!=0) ans++;
             cout<<ans<<'\n';
        }
}
